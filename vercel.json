{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.1" }
    },
    "routes": [
        {
            "src": "^/css/(.*)",
            "dest": "/public/css/$1"
        },
        {
            "src": "^/js/(.*)",
            "dest": "/public/js/$1"
        },
        {
            "src": "^/images/(.*)",
            "dest": "/public/images/$1"
        },
        {
            "src": "^/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "^/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://gerenciador-financeiro-khaki.vercel.app",
        "APP_KEY": "base64:bpmqMWIjz15g5Q/RmuxXXhljV30slTMCkfejJ6bIBGI=",

        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",

        "DB_CONNECTION":"mysql",
        "DB_HOST": "fin_manager.mysql.dbaas.com.br",
        "DB_PORT":"3309",
        "DB_DATABASE": "fin_manager",
        "DB_USERNAME": "fin_manager",
        "DB_PASSWORD": "Bruno@34Un016"

    }
}
